{
  "name": "mtginventory",
  "version": "0.1.0",
  "description": "An inventory tracker for Magic the Gathering with Google Spreadsheets integration",
  "main": "./src/main.js",
  "dependencies": {
    "googleapis": "^32.0.0",
    "mtgsdk": "^0.3.0",
    "nedb": "^1.8.0",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-table-next": "^0.1.15",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4"
  },
  "config": {
    "port": "3000"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "electron": "2.0.5",
    "electron-packager": "^12.1.0",
    "foreman": "^3.0.1"
  },
  "homepage": "file://./",
  "scripts": {
    "start": "cross-env BROWSER=none PORT=$npm_package_config_port react-scripts start",
    "electron": "electron .",
    "dev": "nf start",
    "build-all": "electron-packager ./ MtGInventory --all --out='./build'",
    "build-dev": "electron-packager ./ MtGInventory --out='./build'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "author": "Jaime Wissner <jaime.wissner@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/jairenee/MtG-Inventory.git"
  }
}
